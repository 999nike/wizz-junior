{
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node@3" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/api/(.*)", "dest": "/api/$1.js" }
  ],
  "functions": {
    "api/duo.js": { "maxDuration": 30 },
    "api/ws.js": { "maxDuration": 30 },
    "api/wj.js": { "maxDuration": 30 },
    "api/publish.js": { "maxDuration": 30 }
  }
}